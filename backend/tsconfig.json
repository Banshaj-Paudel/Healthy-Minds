{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "outDir": "./build/",
    "rootDir": "./src/",
    "strict": false,
    "baseUrl": "./",
    "paths": { "*": ["node_modules"] },
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
